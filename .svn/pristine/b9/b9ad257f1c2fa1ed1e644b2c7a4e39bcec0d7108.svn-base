.rinkList { border:1px solid #ddd; margin-bottom:30px; overflow:hidden;}
.rinkList li { padding:0 !important;}
.rinkList li img { width:100%;}
 
 
#footer {clear:both; background:#f4f4f4; border-top:2px solid #e5e5e5; padding:20px 0 30px;  color:#9e9e9e; letter-spacing:-0.6px;}
