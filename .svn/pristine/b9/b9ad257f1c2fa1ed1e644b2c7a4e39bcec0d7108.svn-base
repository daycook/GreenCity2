#footer .page-link div select {float:left; width:79%; height:35px; padding-left:10px; margin-right:3px; background:#fff; border:1px solid #e5e5e5; color:#555;}
#footer .page-link div button {float:left; width:41px; height:35px; border:0 none; background:url(/images/cms/common/go_icon.gif) no-repeat;}
#footer a {color:#eee;}
#footer address {clear:both; padding-bottom:15px;}



