#mainVisual { height:422px; border-bottom:1px solid #e5e5e5; background:#fef1ec;}

/* 바로가기영역 */
.quickWrap { border:1px solid #fff; color:#fff; padding:0 !important; position:relative;}
     .quickWrap span { position:absolute; right:0; bottom:0; z-index:55}
	 .quickWrap span.iconBox01 { background:url(/images/cms/main/quickicon_01.png) no-repeat; width:100px; height:197px; display:block;}
	 .quickWrap span.iconBox02 { background:url(/images/cms/main/quickicon_02.png) no-repeat; width:91px; height:197px; display:block;}
	 .quickWrap span.iconBox03 { background:url(/images/cms/main/quickicon_03.png) no-repeat; width:112px; height:197px; display:block;}
	 .quickWrap span.iconBox04 { background:url(/images/cms/main/quickicon_04.png) no-repeat; width:113px; height:197px; display:block;}
	 
	 
.quickWrap a { color:#fff; display:block;}
.quickWrap a:hover { text-decoration:none; color:#fff;}
.quickWrap dl { padding:30px 0 0 17px;}
.quickWrap dl dt { font-weight:bold; font-size:18px; }
.quickWrap dl dd { font-size:12px; color:#fff; padding-top:20px; margin-right:50%}

    .quick01 { background:url(/images/cms/main/quickBg_01.gif) repeat-x; display:block; height:197px;}
	.quick02 { background:url(/images/cms/main/quickBg_02.gif) repeat-x; display:block; height:197px;}
	.quick03 { background:url(/images/cms/main/quickBg_03.gif) repeat-x; display:block; height:197px;}
	.quick04 { background:url(/images/cms/main/quickBg_04.gif) repeat-x; display:block; height:197px;}

/* 공지사항 */
.newsArea { overflow:hidden;width:100%; margin-bottom:35px;}
.newsArea h4 { font-size:20px; font-weight:bold; color:#000; background:#fafafa; padding:15px; border-bottom:1px solid #ddd;}
.newsArea .noticeList { border:1px solid #ddd; position:relative; }
.newsArea .noticeList ul > li { overflow:hidden; padding:8px 0; margin:0 9px; border-top:1px solid #ddd; background:url(/images/cms/main/list_icon.png) no-repeat 0 20px; line-height:2}
.newsArea .noticeList ul > li a.txt { float:left; padding-left:10px; font-weight:bold; white-space:nowrap; text-overflow:ellipsis;}
.newsArea .noticeList ul > li:first-child { border:0;}
.newsArea .noticeList ul > li .date { float:right; padding-right:2px; font-size:12px;}
.newsArea .noticeList ul > li .con { clear:both; font-size:12px; margin-left:10px; color:#767676;}
.newsArea .noticeList .moreBtn { position:absolute; top:15px; right:10px; background:url(/images/cms/main/more_icon.png) no-repeat; display:block; width:20px; height:20px;}

/* 보도자료 */
.newsArea .bodoList { border:1px solid #ddd; position:relative;}
.newsArea .bodoList ul > li { clear:both; overflow:hidden; padding:6px 0; margin:0 9px; border-top:1px solid #ddd;}
.newsArea .bodoList ul > li:first-child { border:0;}
.newsArea .bodoList ul > li > ol { margin:0 !important}
.newsArea .bodoList ul > li > ol > li { float:left;} 
.newsArea .bodoList ul > li > ol > li > .picA { border:1px solid #ddd; }
.newsArea .bodoList ul > li > ol > li > .picA img { width:100%;}
.newsArea .bodoList ul > li > ol > li.dataA { line-height:2; padding-left:15px;}
.newsArea .bodoList ul > li > ol > li > a.txt { font-weight:bold; white-space:nowrap; text-overflow:ellipsis;}
.newsArea .bodoList ul > li > ol > li .date { padding-right:2px; font-size:12px;}
.newsArea .bodoList ul > li > ol > li .con { clear:both; font-size:12px; color:#767676;}
.newsArea .bodoList .moreBtn { position:absolute; top:15px; right:10px; background:url(/images/cms/main/more_icon.png) no-repeat; display:block; width:20px; height:20px;}

/* 팝업존 */
.newsArea .popupList { position:relative;}
     .popupList ul li img { width:100%; height:100%;}
	 
     div.btnArea {position: absolute; right: 5px; top:14px; z-index:999; height:30px;}
	 div.btnArea ul {float: left;}
     div.btnArea ul li {float: left; padding-right:3px;}
     div.btnArea ul li input { display:block; width:17px; height:17px; border:0; font-size:0}
     div.btnArea ul li input.btn_on {background:url(/images/cms/main/paly_on.png) no-repeat; }
	 div.btnArea ul li input.btn_off {background:url(/images/cms/main/paly_off.png) no-repeat; }
     div.btnArea p {float: left; padding-left:3px;}
	 div.btnArea p input { width:17px; height:17px; display:block; border:0; font-size:0}
	 div.btnArea p input.btn_play { background:url(/images/cms/main/paly_stop.png) no-repeat; }
	 div.btnArea p input.btn_stop { background:url(/images/cms/main/paly_stop.png) no-repeat; }
     
 /* 다채로운이야기 */   
#communityWrap { background:#f9f0cd; padding:43px 0; width:100%; }
  .communityBox .comHead { background:url(/images/cms/common/TopLine-h3.gif) no-repeat center top; text-align:center; font-size:26px; color:#000; font-weight:bold; line-height:2.6}
