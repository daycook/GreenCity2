.navbar { min-height: 0 !important; margin-bottom: 0px !important; border-radius: 0 !important; }
.nav > li > a { padding: 10px 15px !important;}
.caret { margin-left:28px !important;}

/* 왼쪽메뉴 */
.nav>li>a:focus, 
