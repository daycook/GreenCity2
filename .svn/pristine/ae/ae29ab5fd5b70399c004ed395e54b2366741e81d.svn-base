/*기본 테이블*/

.baseTableCont{
	height:100%;
	width: 90%;
	margin: auto;
	min-height: 600px;
	margin-top: 20px;
}

.baseTable{
	width: 100%;
	margin: auto;
	border: 2px solid #e4e4e4;
}
.baseTable tr:HOVER{
	background-color: #F2FFED;
	cursor: pointer;
}

.baseTable th{
	background-color: #e5e5e5;
	color: #000;
	text-align: center;
	height: 40px;
	border-bottom: 2px solid #adadad;
}
.baseTable td{
	height: 40px;
	padding-left: 5px;
	border-bottom: 1px solid #adadad;
}

.baseTable td a{
	color: black;
	cursor: pointer;
}


.subComtTableCont{
	height:100%;
	width: 90%;
	margin: auto;
	min-height: 600px;
	margin-top: 20px;
}

.subComtTable{
	width: 100%;
	margin: auto;
	border: 2px solid #e4e4e4;
}


.subComtTable th{
	background-color: #e5e5e5;
	color: #000;
	text-align: center;
	height: 40px;
	border-bottom: 2px solid #adadad;
}
.subComtTable td{
	height: 40px;
