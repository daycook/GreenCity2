}

/*준비중 화면*/

.preparing{
	min-height: 700px;
	width: 100%; 
	text-align: center;
}

.preparing img{
	width: 60%;
	margin: auto;
}

.preparing a{ 
	color: #ffffff;
	line-height: 700px;
	font-size: 50px;
}